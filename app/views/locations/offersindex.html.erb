<table class="table table-striped">
  <thead>

  </thead>
  <tbody>

  <% @offers.each do |offer| %>

    <tr>

      <td>
        <div style="margin-left: 5px" class="round-image-50">
        </div>
        <%= link_to offer.user.name, user_path(offer.user.id) %></td>
      <td><%= link_to offer.title, offer_path(offer) %></td>
      <td><%= offer.city %></td>

      <td><%= offer.created_at.strftime("%I:%M %p, %b %d") %></td>
      <td>



      </td>
    </tr>

<% end %>


</tbody>

</table>